{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",

  "RateLimit": {
    "TokenBucket": {
      "PolicyName": "TokenPolicy",
      "TokenLimit": 100,
      "QueueLimit": 10,
      "ReplenishmentPeriod": 1,
      "TokensPerPeriod": 10,
      "AutoReplenishment": true
    }
  }
}
