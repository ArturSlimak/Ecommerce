{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },

  "RateLimit": {
    "TokenBucket": {
      "PolicyName": "TokenPolicy",
      "TokenLimit": 5,
      "QueueLimit": 3,
      "ReplenishmentPeriod": 1,
      "TokensPerPeriod": 1,
      "AutoReplenishment": true
    }
  }
}
